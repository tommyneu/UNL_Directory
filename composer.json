{
  "config": {
    "platform": {
      "php": "5.6.1"
    }
  },
  "autoload": {
    "psr-0": {
      "UNL": "src/",
      "Savvy": "vendor/unl/savvy/src"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/unlpear/UNL_Cache_Lite.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/unlpear/UNL_Common.git"
    },
    {
      "type":"package",
      "package": {
          "name": "unl/savvy",
          "version": "master",
          "source": {
              "url": "https://github.com/unl/Savvy.git",
              "type": "git",
              "reference": "master"
          }
      }
    }
  ],
  "require": {
    "unl/savvy": "dev-master",
    "unl/unl_cache_lite": "0.1.*",
    "unl/common": "0.6.*",
    "unl/php-wdn-templates": "^5.3",
    "guzzlehttp/guzzle": "~6.0",
    "jasig/phpcas": "1.3.x-dev",
    "swiftmailer/swiftmailer": "^5.4",
    "slim/csrf": "^0.8.2",
    "deruli/html-minifier": "^0.5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7"
  }
}
